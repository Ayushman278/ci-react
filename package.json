{
  "name": "ci-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "express": "^4.16.2",
    "lodash.debounce": "^4.0.8",
    "lru-cache": "^4.1.1",
    "next": "^4.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "standard": "^10.0.3"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build && git rev-parse HEAD > .next/BUILD_ID",
    "start": "NODE_ENV=production node server.js",
    "lint": "prettier 'utils/**/*.js' 'static/js/**/*.js' 'components/**/*.js' 'pages/**/*.js' '*.js' --write --single-quote --no-semi && standard --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --no-semi",
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "fetch",
      "$"
    ],
    "parser": "babel-eslint"
  }
}
